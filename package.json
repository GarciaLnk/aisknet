{
  "name": "aisknet",
  "displayName": "AIskNet",
  "version": "0.2.0",
  "license": "MIT",
  "description": "Locally-run AI that answers questions from your current webpage",
  "author": "Alberto Garc√≠a <albertogm@usal.es>",
  "scripts": {
    "dev": "plasmo dev",
    "build": "run-s build:*",
    "build:chrome": "plasmo build --hoist",
    "build:firefox": "plasmo build --target=firefox-mv3 --hoist",
    "build:edge": "plasmo build --target=edge-mv3 --hoist",
    "package": "run-s package:*",
    "package:chrome": "plasmo package",
    "package:firefox": "plasmo package --target=firefox-mv3",
    "package:edge": "plasmo package --target=edge-mv3",
    "lint": "prettier --check . && eslint . && svelte-check",
    "format": "prettier --write .",
    "fix": "prettier --write . && eslint . --fix && svelte-check",
    "gendocs": "typedoc --plugin @zamiell/typedoc-plugin-not-exported ./src"
  },
  "dependencies": {
    "@mozilla/readability": "^0.4.4",
    "@plasmohq/messaging": "^0.5.0",
    "@plasmohq/storage": "^1.7.0",
    "@visheratin/tokenizers": "^0.1.5",
    "@visheratin/web-ai": "^1.2.3",
    "html-to-text": "^9.0.5",
    "langchain": "^0.0.114",
    "localforage": "^1.10.0",
    "mark.js": "^8.11.1",
    "ml-distance": "^4.0.1",
    "pdfjs-dist": "^3.8.162",
    "svelte": "^4.1.1"
  },
  "devDependencies": {
    "@plasmohq/prettier-plugin-sort-imports": "^4.0.1",
    "@plasmohq/rps": "^1.8.7",
    "@tailwindcss/typography": "^0.5.9",
    "@types/chrome": "^0.0.241",
    "@types/html-to-text": "^9.0.1",
    "@types/mark.js": "^8.11.8",
    "@types/node": "^20.4.2",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "@zamiell/typedoc-plugin-not-exported": "^0.2.0",
    "autoprefixer": "^10.4.14",
    "daisyui": "^3.3.1",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-svelte": "^2.32.4",
    "eslint-plugin-tsdoc": "^0.2.17",
    "onnxruntime-web": "^1.15.1",
    "plasmo": "^0.81.0",
    "postcss": "^8.4.26",
    "postcss-import": "^15.1.0",
    "prettier": "^3.0.0",
    "prettier-plugin-svelte": "^3.0.0",
    "prettier-plugin-tailwindcss": "^0.4.1",
    "svelte-check": "^3.4.6",
    "svelte-eslint-parser": "^0.32.2",
    "svelte-preprocess": "^5.0.4",
    "tailwindcss": "^3.3.3",
    "typedoc": "^0.24.8",
    "typescript": "^5.1.6"
  },
  "manifest": {
    "permissions": [
      "scripting",
      "activeTab"
    ],
    "content_security_policy": {
      "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self'"
    },
    "web_accessible_resources": [
      {
        "resources": [
          "assets/*.json",
          "assets/ort-wasm-simd.wasm",
          "pdfjs-dist/build/pdf.worker.min.js"
        ],
        "matches": [
          "<all_urls>"
        ]
      }
    ],
    "commands": {
      "_execute_action": {
        "suggested_key": {
          "default": "Alt+A"
        }
      }
    },
    "browser_specific_settings": {
      "gecko": {
        "id": "$FIREFOX_EXT_ID"
      }
    }
  }
}
